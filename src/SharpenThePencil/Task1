int size = 27;                              //Объявление целочисленной переменной и присвоение ей значения 27
String name = "Фидо";                       //Объявление переменнной типа строка и присвоение ей значения "Фидо"
Dog myDog = new Dog(name, size);            //Создание объекта класса Дог (адрес в переменной моя собака)
                                            //с параметрами имя и размер
x = size - 5;                               //Присвоение переменной х значения размера - 5
if (x < 15) myDog.bark(8);                  //Если х меньше 15, то объект собака испольхует метод лаять
while (x > 3) {                             //Пока х больше 3
myDog.play();                               //Объект моя собака использует метод играть
}
int[] numList = (2,4,6,8);                  //Объявление массива целых чисел, значния 2, 4, 6, 8
System.out.print("Привет");                 //Вывод сообещния Привет, без переноса
System.out.print("Собака: " + name);        //Вывод сообщения Собака: и присоединение в строку имени собаки, без переноса
String num = "8";                           //Присвоение переменной типа строка значение "8" (строка)
int z = Integer.parselnt(num);              //Объявление переменной зет и присвоение ей значения строки num
                                            //(перевод строки в число?)
